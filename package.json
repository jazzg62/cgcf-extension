{
	"name": "cgcf",
	"displayName": "cgcf",
	"description": "copy git changes file",
	"version": "0.0.2",
	"author": "https://github.com/jazzg62",
	"publisher": "jazzg62",
	"repository": {
		"type": "git",
		"url": "https://github.com/jazzg62/cgcf-extension"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cgcf.copyChanges"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cgcf.copyChanges",
				"title": "Copy Changes"
			}
		],
		"menus": {
			"scm/resourceState/context": [
				{
					"when": "scmProvider == git",
					"group": "navigation",
					"command": "cgcf.copyChanges"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	},
	"dependencies": {
		"cgcf": "^1.0.2"
	}
}
